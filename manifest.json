{
  "id": "com.dotgears.exporter",
  "name": "UXP_Exporter",
  "version": "1.0.0",
  "main": "index.js",
  "host": [
    {
      "app": "PS",
      "minVersion": "22.0.0"
    }
  ],
  "manifestVersion": 4,
  "entrypoints": [
    {
      "type": "command",
      "id": "runx1",
      "label": {
        "default": "Start Export Action And Dynamic Lighting"
      }
    },   
    {
      "type": "command",
      "id": "runx2",
      "label": {
        "default": "Start Export Action"
      }
    }
  ],
  "icons": [
    {
      "width": 23,
      "height": 23,
      "path": "icons/dark.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "darkest",
        "dark",
        "medium"
      ]
    },
    {
      "width": 23,
      "height": 23,
      "path": "icons/light.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "lightest",
        "light"
      ]
    }
  ]
}